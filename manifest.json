{
    "manifest_version": 2,
    "name": "Mi extensión",
    "description": "Descripción de mi extensión",
    "version": "1.0",
  
    "permissions": [
      "tabs",
      "http://*/*",
      "https://*/*"
    ],
  
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
  
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Título de mi extensión"
    },
    
    "commands": {
        "miComando": {
          "suggested_key": {
            "default": "Ctrl+Shift+F",
            "mac": "MacCtrl+Shift+F"
          },
          "description": "Descripción del comando"
        }
      }
  }
  